{% extends 'SonataAdminBundle::standard_layout.html.twig' %}
{% block stylesheets %}
{{ parent() }}
<link rel="stylesheet" href="{{ asset('bundles/adminvialidad/css/datepicker.css') }}" type="text/css" media="all" />
<link rel="stylesheet" href="{{ asset('bundles/adminvialidad/css/bootstrap-fileupload.min.css') }}" type="text/css" media="all" />
<link rel="stylesheet" href="{{ asset('bundles/adminvialidad/css/vialidad.css') }}" type="text/css" media="all" />
{% endblock %}

{% block javascripts %}
{{ parent() }}
<!--FOSRouting -->
<script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
<script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
<!-- bootstrap js fileupload-->
<script src="{{ asset('bundles/adminvialidad/js/bootstrap-fileupload.min.js') }}" type="text/javascript"></script>
<!-- bootstrap datepicker -->
<script src="{{ asset('bundles/adminvialidad/js/bootstrap-datepicker.js') }}" type="text/javascript"></script>
<script src="{{ asset('bundles/adminvialidad/js/datepicker.conf.js') }}" type="text/javascript"></script>
{% endblock %}
{% block footer %}
    <div class="row-fluid">
        <div class="span2 offset10 pull-right">
            <span class="label"><a href="http://hackathonexpress.com.ar/2013/" rel="noreferrer" style="text-decoration: none; color: black">&copy; Hackathon Express</a></span>
        </div>
    </div>
{% endblock %}
